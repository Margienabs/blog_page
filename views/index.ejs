        <script type="text/javascript">
            $(function(){
                function clearInputs(){
                    $("#name").val("");
                    $("#email").val("");
                    $("#subject").val("");
                    $("#message").val("");
                }
                $(".submit_btn").on( 'click',function(e){
                    $.ajax({
                        type: 'POST',
                        url: '/',
                        data: {
                            "name": $("#name").val(),
                            "email": $("#email").val(),
                            "subject": $("#subject").val(),
                            "message": $("#message").val()
                        },
                        dataType: "json",
                        success: function(response){
                            clearInputs();
                        },
                            error: function() {}
                        })
                });
            })
        </script>
